# Appfile for Shona Learn iOS App

app_identifier("com.shonalearn.app") # The bundle identifier of your app
apple_id(ENV["APPLE_ID"]) # Your Apple email address

team_id(ENV["TEAM_ID"]) # Developer Portal Team ID
itc_team_id(ENV["ITC_TEAM_ID"]) # App Store Connect Team ID (may be different from Developer Portal Team ID)

